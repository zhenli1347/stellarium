msgstr "Tapir . Utara"

# Tupi-Guarani constellation
msgid "Deer"
msgstr "Rusa"

# Tupi-Guarani constellation
msgid "Joykexo"
msgstr "Joykexo"

# Tupi-Guarani constellation, native: Eixu
msgid "Hornet"
